problem:
read mxn matrix and print transpose of given matrix.

source code:
#include <stdio.h>

int main() {
   int m,n;
   scanf("%d %d",&m,&n);
   int a[m][n],i,j;
   for(i=0;i<m;i++)
   {
       for(j=0;j<n;j++)
       {
           scanf("%d",&a[i][j]);
       }
   }
    for(i=0;i<m;i++)
   {
       for(j=0;j<n;j++)
     
           printf("%d ",a[i][j]);
           printf("\n");
       
   }
    for(i=0;i<n;i++)
   {
       for(j=0;j<n;j++)
    
           printf("%d ",a[j][i]);
           printf("\n");
     
   }
   

    return 0;
}
